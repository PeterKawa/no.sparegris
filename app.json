  "flow": {
    "triggers": [
      {
        "title": {
          "en": "Available power changed",
          "no": "Ledig effekt endret seg"
        },
        "titleFormatted": {
          "en": "It is more than [[freePower]] Watts available for other devices for the rest of the hour.",
          "no": "Det er mer enn [[freePower]] Watt ledig for andre enheter for resten av timen."
        },
        "hint": {
          "en": "Use this trigger if you want to control the power of for example an electric car charger.",
          "no": "Bruk denne triggeren til å endre effekten på for eksempel en lader for en elbil."
        },
        "args": [
          {
            "type": "number",
            "name": "freePower",
            "placeholder": {
              "en": "Free Power",
              "no": "Ledig effekt"
            },
            "min": 1,
            "max": 100000,
            "step": 1
          }
        ],
        "tokens": [
          {
            "type": "number",
            "name": "freePower",
            "title": {
              "en": "Free Power",
              "no": "Ledig effekt"
            },
            "example": {
              "en": "Number of additional Watts that can be used for the rest of the hour before the power tariff is exceeded",
              "no": "Antall Watt som kan brukes ekstra resten av timen før effekttariffen blir brutt"
            }
          }
        ],
        "id": "free-power-changed"
      },
      {
        "title": {
          "en": "Unable to limit power",
          "no": "Klarer ikke å begrense strømforbruket"
        },
        "titleFormatted": {
          "en": "Could not reduce power usage. Turn off attached power immediately or the power-tariff for next month will be increased.",
          "no": "Klarer ikke å begrense strømforbruket. Slå av vedlagt effekt umiddelbart ellers vil effekt-tariffen for neste måned øke."
        },
        "hint": {
          "en": "Use this condition as a last resort to limit energy. If the app turned off every controllable device the user can be alerted to do a manual powerdown if this was not enough.",
          "no": "Brukes som en siste utvei for å begrense energiforbruk. Dersom appen har slått av alt kan en melding sendes ut om å redusere forbruket manuelt dersom dette ikke var tilstrekkelig."
        },
        "tokens": [
          {
            "type": "number",
            "name": "excessPower",
            "title": {
              "en": "Excess Power",
              "no": "Overskytende effekt"
            },
            "example": {
              "en": "The amount of Watt that exceeds the power threshold",
              "no": "Antall Watt som overskyter effekttaket"
            }
          }
        ],
        "id": "unable-to-limit-power"
      }
    ],
    "actions": [
      {
        "title": {
          "en": "Change Piggy Bank mode",
          "no": "Endre Sparegris modus"
        },
        "titleFormatted": {
          "en": "Set the Piggy Bank operation mode to [[mode]]",
          "no": "Sett Sparegris driftsmodus til [[mode]]"
        },
        "hint": {
          "en": "Allows you to switch between night, normal and other operation modes",
          "no": "Lar deg bytte mellom natt, normal og andre driftsmoduser"
        },
        "args": [
          {
            "type": "dropdown",
            "name": "mode",
            "values": [
              {
                "id": "0",
                "label": {
                  "en": "App Disabled",
                  "no": "App Inaktiv"
                }
              },
              {
                "id": "1",
                "label": {
                  "en": "Normal",
                  "no": "Normal"
                }
              },
              {
                "id": "2",
                "label": {
                  "en": "Night",
                  "no": "Natt"
                }
              },
              {
                "id": "3",
                "label": {
                  "en": "Away",
